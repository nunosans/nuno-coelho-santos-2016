      <div class="iphone-x-frame project-streams-hero-iphone">
        <img
          src="/images/nuno-coelho-santos-streams-iphone-1x.png"
          srcset="/images/nuno-coelho-santos-streams-iphone-2x.png 2x,
                  /images/nuno-coelho-santos-streams-iphone-3x.png 3x"
          alt="Streams App Icon"
          width=""
          height=""
        >
      </div>
    </div>

