<article class="project project-streams">

  <section class="emphasize">
    <p>
      <%= t(:streams_title) %>
    </p>
  </section>

  <main>

    <div class="project-intro">
      <h6><%= t(:label_role) %></h6>
      <p>
        <%= t(:streams_role) %>
      </p>
      <h6><%= t(:label_date) %></h6>
      <p>
        <%= t(:streams_date) %>
      </p>
      <h6><%= t(:label_website) %></h6>
      <p>
        <%= t(:streams_website) %>
      </p>
    </div>

    <div class="project-item project-streams-app-icon">
      <img
        class="lazy-load"
        src="/images/nuno-coelho-santos-streams-app-icon-1x.png"
        srcset="/images/nuno-coelho-santos-streams-app-icon-2x.png 2x,
                /images/nuno-coelho-santos-streams-app-icon-3x.png 3x"
        alt="Streams App Icon"
        width="128"
        height="128"
      >
      <p>
        <%= t(:streams_body_1) %>
      </p>
    </div>

    <div class="project-item project-streams-hero">
      <img
        class="lazy-load project-streams-hero-ipad"
        src="/images/nuno-coelho-santos-streams-ipad-1x.png"
        srcset="/images/nuno-coelho-santos-streams-ipad-2x.png 2x,
                /images/nuno-coelho-santos-streams-ipad-3x.png 3x"
        alt="Streams for iPad"
        width=""
        height=""
