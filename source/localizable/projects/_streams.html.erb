          <td colspan="3">
            <%= t(:label_papers) %>
          </td>
        </thead>
        <tbody>
          <tr>
            <td>2019</td>
            <td>Nature</td>
            <td><a target="_blank" href="https://www.nature.com/articles/s41586-019-1390-1"><%= t(:streams_paper_1) %></a></td>
          </tr>
          <tr>
            <td>2019</td>
            <td>npj Digital Medicine</td>
            <td><a target="_blank" href="https://www.nature.com/articles/s41746-019-0100-6"><%= t(:streams_paper_2) %></a></td>
          </tr>
        </tbody>
        <thead>
          <td colspan="3">
            <%= t(:label_press) %>
          </td>
        </thead>
        <tbody>
          <tr>
            <td>2019</td>
            <td>The Times</td>
            <td><a target="_blank" href="https://www.thetimes.co.uk/article/4d0f9830-d3b7-11e9-aa6d-16cb9f989e55"><%= t(:streams_press_1) %></a></td>
          </tr>
          <tr>
